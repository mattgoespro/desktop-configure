{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react-jsx",
    "lib": ["dom", "es2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dll",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es2021"
  },
  "include": ["src/**/*"]
}
