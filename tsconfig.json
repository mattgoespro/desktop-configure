{
  "compilerOptions": {
    "incremental": true,
    "target": "es2021",
    "module": "commonjs",
    "lib": ["dom", "es2021"],
    "jsx": "react-jsx",
    "sourceMap": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "dll",
    "paths": {
      "image-to-icon/event-handler": ["src/main/apps/image-to-icon/event-handlers/index.ts"],
      "image-to-icon/events/*": ["src/main/apps/image-to-icon/events/*"],
      "window-event-emitter": ["src/main/shared/window-event-emitter.ts"],
      "file-picker-dialog": ["src/main/shared/file-picker-dialog.ts"],
      "utils/*": ["src/main/utils.ts"],
      "preload": ["src/main/preload.ts"],
      "uuid-four": ["src/renderer/App/Utils/uuid-four.ts"],
      "shared/*": ["src/renderer/App/Components/Shared/*"],
      "redux/*": ["src/renderer/App/Redux/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["release/build", "release/app/dist"]
}
