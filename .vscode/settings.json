{
  "[dockerfile]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[nginx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "auto-close-tag.activationOnLanguage": [
    "html",
    "xml",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "html"
  ],
  "css.format.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.fixAll.scss": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.enable": true,
  "eslint.format.enable": false,
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "eslint.run": "onType",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "files.associations": {
    ".eslintignore": "ignore"
  },
  "files.eol": "\n",
  "files.exclude": {
    ".git": true,
    "node_modules": true
  },
  "files.watcherExclude": {
    ".git": true,
    "node_modules": true
  },
  "html.format.enable": false,
  "javascript.format.enable": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "markdown.occurrencesHighlight.enabled": true,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.hidesExplorerArrows": true,
  "npm-intellisense.importQuotes": "\"",
  "npm-intellisense.showBuildInLibs": true,
  "npm-outdated.majorUpdateProtection": false,
  "npm.exclude": "release/**",
  "prettier.configPath": ".prettierrc.js",
  "scss.completion.completePropertyWithSemicolon": true,
  "scss.completion.triggerPropertyValueCompletion": true,
  "scss.format.enable": false,
  "scss.showErrors": true,
  "scssFormatter.printWidth": 100,
  "scssFormatter.singleQuote": false,
  "auto-close-tag.disableOnLanguage": ["javascript", "typescript", "plaintext"],
  "search.exclude": {
    "*.{css,sass,scss}.d.ts": true,
    "dll": true,
    ".git": true,
    "dist": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "package-lock.json": true,
    "release/{build,app/dist}": true
  },
  "typescript.format.enable": false,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.iconTheme": "material-icon-theme",
  "react-native-tools.networkInspector.consoleLogsColorTheme": "Dark",
  "react-native-tools.projectRoot": "${workspaceFolder}/src/renderer"
}
